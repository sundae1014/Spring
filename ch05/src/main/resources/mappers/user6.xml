<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.ch05.mapper.User6Mapper">
    <insert id="insertUser6">
        INSERT INTO USER6 VALUES (#{user_id},#{name},#{birth},#{age})
    </insert>
    <select id="selectAllUser6" resultType="kr.co.ch05.dto.User6DTO">
        SELECT * FROM USER6
    </select>
    <select id="selectUser6" resultType="kr.co.ch05.dto.User6DTO">
        SELECT * FROM USER6 WHERE user_id=#{user_id}
    </select>
    <update id="updateUser6">
        UPDATE USER6 SET
            name=#{name},
            birth=#{birth},
            age=#{age}
        WHERE
            user_id=#{user_id}
    </update>
    <delete id="deleteUser6">
        DELETE FROM USER6 WHERE user_id=#{user_id}
    </delete>
</mapper>