<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.ch05.mapper.User5Mapper">
    <insert id="insertUser5">
        INSERT INTO USER5 VALUES (#{user_id},#{name},#{birth},#{age})
    </insert>
    <select id="selectAllUser5" resultType="kr.co.ch05.dto.User5DTO">
        SELECT * FROM USER5
    </select>
    <select id="selectUser5" resultType="kr.co.ch05.dto.User5DTO">
        SELECT * FROM USER5 WHERE user_id=#{user_id}
    </select>
    <update id="updateUser5">
        UPDATE USER5 SET
            name=#{name},
            birth=#{birth},
            age=#{age}
        WHERE
            user_id=#{user_id}
    </update>
    <delete id="deleteUser5">
        DELETE FROM USER5 WHERE user_id=#{user_id}
    </delete>
</mapper>